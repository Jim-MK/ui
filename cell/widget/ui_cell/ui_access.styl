@import "../../base/fn"

.ui_cells_access
  .ui_cell:not(.no_access)
    //在cell_access和其它类型的cell混着用的场景下，其它cell要加no_access，避免有点击态
    setTapColor()
    &:active
      background-color  #ECECEC


  a.ui_cell
    color inherit

  .ui_cell_ft
    &:after
      content " "
      setArrow_Wap(right, 6px, #C8C8CD, 2px)
      top -1px
      margin-left .3em



